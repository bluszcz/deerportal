	@$(QMAKE) -nocache QMAKE_CFLAGS_RELEASE=-g\ -O2\ -fstack-protector\ --param=ssp-buffer-size=4\ -Wformat\ -Werror=format-security\ -D_FORTIFY_SOURCE=2 QMAKE_CFLAGS_DEBUG=-g\ -O2\ -fstack-protector\ --param=ssp-buffer-size=4\ -Wformat\ -Werror=format-security\ -D_FORTIFY_SOURCE=2 QMAKE_CXXFLAGS_RELEASE=-g\ -O2\ -fstack-protector\ --param=ssp-buffer-size=4\ -Wformat\ -Werror=format-security\ -D_FORTIFY_SOURCE=2 QMAKE_CXXFLAGS_DEBUG=-g\ -O2\ -fstack-protector\ --param=ssp-buffer-size=4\ -Wformat\ -Werror=format-security\ -D_FORTIFY_SOURCE=2 QMAKE_LFLAGS_RELEASE=-Wl,-Bsymbolic-functions\ -Wl,-z,relro QMAKE_LFLAGS_DEBUG=-Wl,-Bsymbolic-functions\ -Wl,-z,relro QMAKE_STRIP=: PREFIX=/usr -o Makefile deerportal.pro

qmake_all: FORCE

dist: 
	@test -d .tmp/deerportal1.0.0 || mkdir -p .tmp/deerportal1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/deerportal1.0.0/ && (cd `dirname .tmp/deerportal1.0.0` && $(TAR) deerportal1.0.0.tar deerportal1.0.0 && $(COMPRESS) deerportal1.0.0.tar) && $(MOVE) `dirname .tmp/deerportal1.0.0`/deerportal1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/deerportal1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) debian/deerportal/opt/deerportal/$(TARGET) 
	-$(DEL_FILE) Makefile


check: first

